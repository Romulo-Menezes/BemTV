@layout('layouts/main')
@set('title', 'Home')

@section('body')
@!alertSuccess()
@!alertError()

@!pageNav({videos, pagination})
<div class="flex flex-wrap items-center justify-center">
  @each(row in videos)
  @!video.card({
    id: row.id,
    url_code: row.url_code,
    title: row.title.length > 38 ? row.title.substring(0,35) + '...' : row.title,
  })
  @endeach
</div>
<div class="mb-6">
  @!pageNav({videos, pagination})
</div>
@endsection
